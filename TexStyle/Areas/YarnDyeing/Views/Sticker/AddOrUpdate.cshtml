@model StickerViewModel

@{
    ViewData["Title"] = "Add Or Update";


    List<SelectListItem> LPSList = ViewBag.LPSList;
}
@*<form action="/@AreaConstants.YARN_DYEING.Name/MachineType/AddOrUpdate" method="post" enctype="multipart/form-data">*@

<form asp-area="@(AreaConstants.YARN_DYEING.Name)" asp-controller="Sticker" asp-action="AddOrUpdate" method="post" enctype="multipart/form-data">
    <div class="form-horizontal">

        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        @Html.HiddenFor(m => m.Id)
        <div class="form-group">
            <label asp-for="LPSId" class="control-label"></label>
            <input asp-for="LPSId" class="form-control" />
            <span asp-validation-for="LPSId" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="Cons" class="control-label"></label>
            <input asp-for="Cons" class="form-control" />
            <span asp-validation-for="Cons" class="text-danger"></span>
        </div>
    </div>
    <div class="modal-footer">
        <button type="submit" class="btn btn-primary">Save</button>
        <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
    </div>
</form>
