                            <div class="main-body">
                                <div class="page-wrapper">
                                    <div class="page-header d-flex justify-content-between">
                                        <div class="page-header-title">

                                            @{
                                                var areStr = ViewContext.RouteData.Values["area"]?.ToString();
                                                var areaName = "";
                                                if (!string.IsNullOrEmpty(areStr))
                                                {
                                                    areaName = System.Text.RegularExpressions.Regex.Replace(areStr, "([a-z])([A-Z])", "$1 $2");
                                                    <h4 class="no-print">@areaName / @ViewData["Title"] </h4>
                                                }

                                                FilterOptionsViewModel FilterOpts = ViewBag.FilterOpts;
                                            }

                                        </div>
                                        @if (FilterOpts != null)
                                        {
                                            <div class="d-flex">
                                                <div class="form-group m-0 d-flex align-items-center mr-1">
                                                    <label class="control-label">From: </label>
                                                    <input id="sd" class="form-control form-control-sm" type="date" value="@FilterOpts.sd" />
                                                </div>
                                                <div class="form-group m-0 d-flex align-items-center mr-1">
                                                    <label class="control-label">To: </label>
                                                    <input id="ed" class="form-control form-control-sm" type="date" value="@FilterOpts.ed" />
                                                </div>
                                                <div class="form-group m-0 d-flex align-items-center">
                                                    <a href="#" id="FilterBtn" class="btn btn-primary btn-sm">Filter</a>
                                                </div>
                                            </div>
                                        }

                                        @*<div class="page-header-breadcrumb">
