    @RenderSection("scripts", required: false)

    <script>
        $(".select2").select2();
        $(".dataTable").DataTable();
        $(".sdataTable").DataTable({
            scrollX: 600,
            scrollY: true,
            autoWidth: true,
        });

        $(document).on("click", '.create-js,.update-js', function (e) {
            e.preventDefault();
            var mtitle = $(modal).children().find(".modal-title");
            var mbody = $(modal).children().find(".modal-body");
            var url = $(this).data("action").replace('#', '');

            var btn = $(this);
            btn.prop('disabled', true);
            var BtnText = btn.text();
            btn.html(`<i class="fa fa-refresh fa-spin text-white"></i>`);

            mtitle.text($(this).data("title"))
            $.ajax({
                url: url,
                method: 'GET'
            }).done(function (res) {
                btn.text(BtnText);
                mbody.html(res);

                var form = $("form");
                form.removeData('validator');
                form.removeData('unobtrusiveValidation');
                $.validator.unobtrusive.parse(form);

                $(".select2").select2({
                    dropdownParent: $(modal)
                });

                $(".m-colorpicker").spectrum({
                    flat: true,
                    showInput: true
                });

                // Mini-color js start
                $('.demo').each(function () {
                    //
                    // Dear reader, it's actually very easy to initialize MiniColors. For example:
                    //
                    //  $(selector).minicolors();
                    //
                    // The way I've done it below is just for the demo, so don't get confused
                    // by it. Also, data- attributes aren't supported at this time...they're
                    // only used for this demo.
                    //
                    $(this).minicolors({
                        control: $(this).attr('data-control') || 'hue',
                        defaultValue: $(this).attr('data-defaultValue') || '',
                        format: $(this).attr('data-format') || 'hex',
                        keywords: $(this).attr('data-keywords') || '',
                        inline: $(this).attr('data-inline') === 'true',
                        letterCase: $(this).attr('data-letterCase') || 'lowercase',
                        opacity: $(this).attr('data-opacity'),
                        position: $(this).attr('data-position') || 'bottom left',
                        swatches: $(this).attr('data-swatches') ? $(this).attr('data-swatches').split('|') : [],
                        change: function (value, opacity) {
                            if (!value) return;
                            if (opacity) value += ', ' + opacity;
                            if (typeof console === 'object') {
                                console.log(value);
                            }
                        },
                        theme: 'bootstrap'
                    });
                });
                // Mini-color js ends

                $(modal).modal();
                btn.prop('disabled', false);

            }).fail(function () {
                btn.text(BtnText);
                swal("Failed", "Request Failed. Try later.", "error");
                btn.prop('disabled', false);

            });

        });

        $(function () {

            $("form").submit(function () {
                if (!$(this).valid()) return false;

                $("form *:not(option)").prop("disabled", false);
            });

            $("#sd, #ed").change(function () {
                var sd = $("#sd").val();
                var ed = $("#ed").val();

                var url = window.location.href.substring(0, window.location.href.lastIndexOf("?")) + `?sd=${sd}&ed=${ed}`;

                $("#FilterBtn").attr("href", url);
            });

            $(document).on("click", ".item-delete-btn", function () {
                var url = $(this).attr("data-delete-url");
                var table = $(this).parents("table").DataTable();
                var parentRow = $(this).closest("tr");

                swal({
                    title: "Are you sure?",
                    text: "You will not be able to undo this action!",
                    type: "warning",
                    showCancelButton: true,
                    confirmButtonClass: "btn-danger",
                    confirmButtonText: "Yes",
                    cancelButtonText: "Cancel",
                    closeOnConfirm: false,
                    closeOnCancel: false
                },
                    function (isConfirm) {
                        if (isConfirm) {
                            $.ajax({
                                type: "POST",
                                url: url,
                                success: function (result) {
                                    table.row(parentRow)
                                        .remove()
                                        .draw();
                                    swal("Deleted!", "Item has been deleted.", "success");

                                }, error: function () {
                                    swal("Failed", "Request Failed. Try later.", "error");
                                }
                            });

                        } else {
                            swal("Cancelled", "Operation has been canceled.", "error");
                        }
                    });
            });
        })
    </script>


    <script>
        $(function () {

            $(document).on("input", "#QtyDr", function () {
                $("#QtyCr").val("").attr("placeholder", "Unset");
                $("#Amount").val(parseInt($("#QtyDr").val()) * parseInt($("#Rate").val()));
            });

            $(document).on("input", "#QtyCr", function () {
                $("#QtyDr").val("").attr("placeholder", "Unset");
                $("#Amount").val(parseInt($("#QtyCr").val()) * parseInt($("#Rate").val()));

            });

            $(document).on("input", "#Rate", function () {
                var CR = $("#QtyCr").val();
                var DR = $("#QtyDr").val();

                var qty = 0;

                if (CR != "") {
                    qty = parseInt(CR);
                }
                else if (DR != "") {
                    qty = parseInt(DR);
                }

                $("#Amount").val(qty * parseInt($(this).val()));
            });


            $(document).on("change", "#ChemicalId", function (e) {
                if ($(this).val() == null) // Value of Select is null instead of -1 because -1 option is disabled
                {
                    return;
                }

                $("#DyeId").val("-1").trigger("change");

            })

            $(document).on("change", "#DyeId", function (e) {
                if ($(this).val() == null) // Value of Select is null instead of -1 because -1 option is disabled
                {
                    return;
                }

                $("#ChemicalId").val("-1").trigger("change");

            })
        })
    </script>

